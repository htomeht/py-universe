<localization language="jp" encoding="utf-8">
<verbs>
	<!-- どうし - 動詞 -->
    <verb lbl="GO">		<g  reg="行く" /></verb>
    <verb lbl="ENTER">	<g  reg="入る"  /></verb>
    <verb lbl="OUT">		<g reg="置く" /></verb>    
    <verb lbl="GET">      <g  reg="取る" />
								<g reg="得る" /></verb>
<!-- etc. ... -->
</verbs>

<adverbs>
	<!-- ふくし - 副詞 -->
	<advb lbl="WEAKLY">	  <g reg="弱々しく" />	</advb>
	<advb lbl="STRONGLY">	<g reg="強く" />		</advb>
</adverbs>

<nouns>
	<!-- めいし - 名詞 -->
    <noun lbl="BOOK">   <g  reg="本"   />    </noun>
    <noun lbl="PEN">    <g  reg="ペン" />    </noun>
<!-- etc. ... -->
</nouns>

<adjectives>
	<!-- けいようし と けいようどうし - 形容詞 と 形容動詞 -->
    <adje lbl="RED"    ><g  reg="赤い"/>    </adje>
    <adje lbl="BLUE"   ><g  reg="青い"/>     </adje>
<!-- etc. ... -->
</adjectives>

<declensions>
	<!-- Declension particles - ぶんぽうのじょし - 文法の助詞 -->
	<decl lbl="NOM">	<g reg="は" /> 
							<g reg="が" /> </decl>
	<decl lbl="ACC">	<g reg="を" /> </decl>
	<decl lbl="DAT">	<g reg="え" />
							<g reg="に" /> </decl>
	<decl lbl="GEN">	<g reg="の" />	</decl>
	<decl lbl="INS">	<g reg="で" />	</decl>
</declensions>

<prepositions>
	<!--                        【ポロジの】ばしょ の じょし -->
	<!-- Topological Prepositions - 【トポロジの】場所の助詞 -->
	<prep lbl="FRONT"> 
			<g reg="の前に" />
			<g reg="の前で" />
	</prep>
	<prep lbl="BACK">
			<g reg="の後ろに" />
			<g reg="の後ろで" />
	</prep>
	<prep lbl="ON_TOP">
			<g reg="の上に" />
			<g reg="の上で" />
	</prep>
	<prep lbl="UNDER">
			<g reg="の下に" />
			<g reg="の下で" />
	</prep>
	<prep lbl="TO_LEFT">
			<g reg="の左に" />
			<g reg="の左で" />
	</prep>
	<prep lbl="TO_RIGHT">
			<g reg="の右に" />
			<g reg="の右で" />
	</prep>
	<prep lbl="INSIDE">
			<g reg="の中に" />
			<g reg="の中で" />
			<g reg="に" />
			<g reg="で" />
	</prep>
</prepositions>
<punctuation>
	<punc lbl="ADDRESS"><g chars="、。"/></punc>
	<punc lbl="CONJ"><g chars="、。"/></punc>
</punctuation>
<conjunctions punctuation="Y">
	<conj><g reg="し" />
			<g reg="が" />
	</conj>
</conjunctions>
</localization>

